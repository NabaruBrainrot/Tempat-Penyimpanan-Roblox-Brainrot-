---Ghost Gui UI Library
loadstring(game:HttpGet('https://raw.githubusercontent.com/GhostPlayer352/UI-Library/refs/heads/main/Ghost%20Gui'))()
game.CoreGui.GhostGui.MainFrame.Title.Text = "Ghost Hub"
---



AddContent("Switch", "Instan Steal", [[
-- ======================================================
-- GUI TEMPLATE
-- ======================================================
local ScreenGui = Instance.new("ScreenGui")    
local Frame = Instance.new("Frame")    
local Button = Instance.new("TextButton")    
local Label = Instance.new("TextLabel")    
local UICorner_Frame = Instance.new("UICorner")    
local UICorner_Button = Instance.new("UICorner")    
local UICorner_Label = Instance.new("UICorner")    

ScreenGui.Name = "DeliveryTouchGUI"    
ScreenGui.Parent = game.CoreGui    
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling    

-- Frame utama    
Frame.Parent = ScreenGui    
Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)    
Frame.Position = UDim2.new(0, 20, 0, 100)    
Frame.Size = UDim2.new(0, 140, 0, 90)    
Frame.Active = true    
Frame.Draggable = true    
UICorner_Frame.CornerRadius = UDim.new(0, 10)    
UICorner_Frame.Parent = Frame    

-- Tombol    
Button.Parent = Frame    
Button.BackgroundColor3 = Color3.fromRGB(0, 170, 255)    
Button.Position = UDim2.new(0, 10, 0, 45)    
Button.Size = UDim2.new(0, 120, 0, 30)    
Button.Font = Enum.Font.GothamBold    
Button.Text = "Click Steal"    
Button.TextColor3 = Color3.fromRGB(255, 255, 255)    
Button.TextSize = 12    
Button.TextWrapped = true    
UICorner_Button.CornerRadius = UDim.new(0, 8)    
UICorner_Button.Parent = Button    

-- Label status    
Label.Parent = Frame
Label.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Label.Position = UDim2.new(0, 10, 0, 10)
Label.Size = UDim2.new(0, 120, 0, 25)
Label.Font = Enum.Font.GothamBold
Label.Text = "Ready"
Label.TextColor3 = Color3.fromRGB(255, 255, 255)
Label.TextSize = 12
UICorner_Label.CornerRadius = UDim.new(0, 8)
UICorner_Label.Parent = Label

-- ======================================================
-- LOGIC
-- ======================================================
local player = game.Players.LocalPlayer
local isProcessing = false

-- Ambil HumanoidRootPart
local function getHRP()
    if not player.Character then
        player.CharacterAdded:Wait()
    end
    return player.Character:WaitForChild("HumanoidRootPart")
end

-- Cek apakah plot ini milik kita (TextLabel)
local function isMyBase(plot)
    local ok, textLabel = pcall(function()
        local sign = plot:WaitForChild("PlotSign", 1)
        local sg = sign:WaitForChild("SurfaceGui", 1)
        local frame = sg:WaitForChild("Frame", 1)
        return frame:WaitForChild("TextLabel", 1)
    end)
    if not ok or not textLabel then return false end
    local text = tostring(textLabel.Text or ""):lower()
    local username = tostring(player.Name or ""):lower()
    local displayname = tostring(player.DisplayName or ""):lower()
    return text:find(username) or text:find(displayname)
end

-- Fungsi teleport part ke player lalu kembali
local function touchPartToPlayer(part)
    if not part or not part:IsA("BasePart") then return false end
    local hrp = getHRP()
    local originalCFrame = part.CFrame
    local originalParent = part.Parent
    local anchoredState = part.Anchored

    -- Teleport part ke HumanoidRootPart
    part.Anchored = true
    part.CFrame = hrp.CFrame
    if part.Parent ~= workspace then
        part.Parent = workspace
    end

    -- Fire touch ke badan player
    firetouchinterest(hrp, part, 0) -- TouchBegan
    task.wait(0.1)
    firetouchinterest(hrp, part, 1) -- TouchEnded

    -- Kembalikan part ke posisi asal
    part.CFrame = originalCFrame
    part.Parent = originalParent
    part.Anchored = anchoredState

    return true
end

-- Main Action
local function fireCollectZone()
    if isProcessing then return end
    isProcessing = true
    Label.Text = "Processing..."

    task.wait(0.1) -- Delay 0.1 detik

    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
        player.CharacterAdded:Wait()
    end

    -- Scan semua plot
    local found = false
    for _, plot in ipairs(workspace.Plots:GetChildren()) do
        if isMyBase(plot) then
            local deliveryPart = plot:FindFirstChild("DeliveryHitbox")
            if deliveryPart then
                local success = touchPartToPlayer(deliveryPart)
                if success then
                    Label.Text = "Success"
                else
                    Label.Text = "Failed"
                end
            else
                Label.Text = "Failed"
            end
            found = true
            break
        end
    end

    if not found then
        Label.Text = "Failed"
    end

    task.wait(0.1)
    Label.Text = "Ready"
    isProcessing = false
end

-- Klik tombol GUI
Button.MouseButton1Click:Connect(fireCollectZone)
]], [[
-- ======================================================
-- CLEAR GUI SCRIPT
-- ======================================================
local gui = game.CoreGui:FindFirstChild("DeliveryTouchGUI")
if gui then
	gui:Destroy()
end
]])





AddContent("Switch", "Noclip", [[
-- Script ON Noclip
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

_G.Noclip = true

RunService.Stepped:Connect(function()
    if _G.Noclip then
        local char = player.Character
        if char then
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") and part.CanCollide == true then
                    part.CanCollide = false
                end
            end
        end
    end
end)
]],[[
-- Script OFF Noclip
_G.Noclip = false

-- Reset semua part agar bisa collide lagi
local player = game:GetService("Players").LocalPlayer
if player.Character then
    for _, part in pairs(player.Character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = true
        end
    end
end
]])






AddContent("Switch", "Infjump", [[
-- ======================================================
-- Script ON Infinite Jump (dorong ke atas)
-- ======================================================
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

_G.InfJump = true

-- Fungsi untuk dorong ke atas
local function pushUp()
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        local hrp = char.HumanoidRootPart
        -- Dorong ke atas dengan menambah velocity Y
        hrp.Velocity = Vector3.new(hrp.Velocity.X, 50, hrp.Velocity.Z)
    end
end

-- Saat tombol jump ditekan
UserInputService.JumpRequest:Connect(function()
    if _G.InfJump then
        pushUp()
    end
end)
]],[[
-- ======================================================
-- Script OFF Infinite Jump
-- ======================================================
_G.InfJump = false
]])




AddContent("Switch", "Auto Lock", [[
-- ======================================================
-- AUTO FIRE TOUCH (0.1 DETIK)
-- Toggle ON/Off script ini berjalan otomatis setiap 0.1 detik
-- ======================================================

getgenv().AutoHitboxEnabled = true

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Update character setiap respawn
player.CharacterAdded:Connect(function(char)
	character = char
	character:WaitForChild("HumanoidRootPart")
	print("üîÑ Character respawn detected, character updated!")
end)

-- Cek apakah plot ini milik kita
local function isMyBase(plot)
	local ok, textLabel = pcall(function()
		local sign = plot:WaitForChild("PlotSign", 1)
		local sg = sign:WaitForChild("SurfaceGui", 1)
		local frame = sg:WaitForChild("Frame", 1)
		return frame:WaitForChild("TextLabel", 1)
	end)
	if not ok or not textLabel then return false end
	local text = tostring(textLabel.Text or ""):lower()
	local username = tostring(player.Name or ""):lower()
	local displayname = tostring(player.DisplayName or ""):lower()
	return text:find(username) or text:find(displayname)
end

-- Fungsi fire touch ke Hitbox
local function fireHitbox(hitbox)
	if hitbox and hitbox:IsA("BasePart") then
		local hrp = character and character:FindFirstChild("HumanoidRootPart")
		if hrp then
			firetouchinterest(hrp, hitbox, 0)
			task.wait(0.1)
			firetouchinterest(hrp, hitbox, 1)
			print("‚ö° Hitbox disentuh:", hitbox:GetFullName())
		else
			print("‚ùå HRP belum siap!")
		end
	else
		print("‚ùå Hitbox tidak ditemukan!")
	end
end

-- Main loop: scan base-mu & fire touch setiap 0.1 detik
spawn(function()
	while getgenv().AutoHitboxEnabled do
		task.wait(0.1)
		for _, plot in ipairs(workspace.Plots:GetChildren()) do
			if isMyBase(plot) then
				local hitbox = plot:FindFirstChild("Purchases") and
					plot.Purchases:FindFirstChild("PlotBlock") and
					plot.Purchases.PlotBlock:FindFirstChild("Hitbox")
				if hitbox then
					fireHitbox(hitbox)
				end
				break -- hentikan loop setelah menemukan base-mu
			end
		end
	end
end)
]], [[
-- ======================================================
-- TOGGLE OFF
-- ======================================================
getgenv().AutoHitboxEnabled = false
print("‚ùå Auto Fire Hitbox stopped.")
]])