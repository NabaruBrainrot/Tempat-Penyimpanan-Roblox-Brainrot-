]], [[
-- ==== BLOCK: ON (toggle ON ‚Üí GUI DITUTUP) ====
if game.CoreGui:FindFirstChild("TeleportGUI") then
    game.CoreGui.TeleportGUI:Destroy()
    print("‚ùå Teleport GUI ditutup.")
end
]])



AddContent("Switch", "speed", [[
-- ==== BLOCK: OFF (toggle OFF ‚Üí Speed CEPAT) ====
local player = game.Players.LocalPlayer
if player and player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
    player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 50
    print("‚ö° Speed diaktifkan jadi 50 (toggle OFF).")
else
    warn("‚ö†Ô∏è Tidak dapat menemukan karakter atau humanoid.")
end
]], [[
-- ==== BLOCK: ON (toggle ON ‚Üí Speed NORMAL) ====
local player = game.Players.LocalPlayer
if player and player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
    player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = 23
    print("‚ùå Speed dikembalikan ke normal (toggle ON).")
end
]])
