---Ghost Gui UI Library
loadstring(game:HttpGet('https://raw.githubusercontent.com/GhostPlayer352/UI-Library/refs/heads/main/Ghost%20Gui'))()
game.CoreGui.GhostGui.MainFrame.Title.Text = "Fish it"
---






AddContent("Switch", "Instan Fishing", [[
-- ==== BLOCK: OFF (Toggle MATI ‚Üí Script AKTIF) ====
_G.AutoFishingEnabled = true
print("üé£ Auto Fishing Dinyalakan (Toggle OFF)")

-- Hentikan loop lama kalau masih jalan
if _G.AutoFishingLoop and coroutine.status(_G.AutoFishingLoop) ~= "dead" then
    task.cancel(_G.AutoFishingLoop)
end

-- Simpan posisi awal saat pertama kali aktif
if not _G.OriginalCFrame and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
    _G.OriginalCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end

_G.AutoFishingLoop = task.spawn(function()
    local lastResetTime = os.clock()

    while _G.AutoFishingEnabled do
        local now = os.clock()


        if now - lastResetTime >= 120 then
            local player = game.Players.LocalPlayer
            if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local lastPos = player.Character.HumanoidRootPart.CFrame
                print("‚ôªÔ∏è Reset karakter di lokasi saat ini...")

                -- Reset cepat (bunuh karakter)
                player.Character:FindFirstChildOfClass("Humanoid"):TakeDamage(9999)

                -- Tunggu karakter baru muncul
                player.CharacterAdded:Wait()
                repeat task.wait() until player.Character:FindFirstChild("HumanoidRootPart")

                -- Teleport kembali ke posisi sebelum reset
                task.wait(0)
                player.Character:WaitForChild("HumanoidRootPart").CFrame = lastPos
                print("üìç Respawn di lokasi sebelum reset")
            end
            lastResetTime = os.clock()
        end

        -- === Logic Auto Fishing Asli ===
        -- Langkah 1
        local args1 = { 1 }
        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
            :WaitForChild("net"):WaitForChild("RE/EquipToolFromHotbar")
            :FireServer(unpack(args1))
        print("‚ñ∂Ô∏è Langkah 1 dijalankan")

        task.wait(0.1)

        -- Langkah 2
        local args2 = { 1760498391.426017 }
        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
            :WaitForChild("net"):WaitForChild("RF/ChargeFishingRod")
            :InvokeServer(unpack(args2))
        print("‚ñ∂Ô∏è Langkah 2 dijalankan")

        task.wait(0.1)

        -- Langkah 3
        local args3 = { -0.5718746185302734, 0.9997670786545603 }
        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
            :WaitForChild("net"):WaitForChild("RF/RequestFishingMinigameStarted")
            :InvokeServer(unpack(args3))
        print("‚ñ∂Ô∏è Langkah 3 dijalankan")

        task.wait(2.5)

        -- Langkah 4
        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
            :WaitForChild("net"):WaitForChild("RE/FishingCompleted")
            :FireServer()
        print("‚úÖ Langkah 4 dijalankan (FishingCompleted)")

        -- Jika toggle sudah diubah ke ON (dimatikan), hentikan setelah siklus ini
        if not _G.AutoFishingEnabled then
            print("‚ö†Ô∏è Auto Fishing Berhenti Setelah Selesai Satu Siklus")
            break
        end

        task.wait(0.01)
    end
end)
]], [[
-- ==== BLOCK: ON (Toggle HIDUP ‚Üí Script NONAKTIF) ====
_G.AutoFishingEnabled = false
print("üõë Auto Fishing Diminta Berhenti (akan berhenti setelah siklus selesai)")
]])








AddContent("Switch", "Auto Sell", [[
-- ==== BLOCK: OFF (ketika toggle OFF ‚Üí Script AKTIF) ====
_G.AutoSellEnabled = true
print("üí∞ Auto Sell diaktifkan (toggle OFF)")

-- Hentikan loop lama jika masih berjalan
if _G.AutoSellLoop and coroutine.status(_G.AutoSellLoop) ~= "dead" then
    task.cancel(_G.AutoSellLoop)
end

_G.AutoSellLoop = task.spawn(function()
    while _G.AutoSellEnabled do
        game:GetService("ReplicatedStorage")
            :WaitForChild("Packages"):WaitForChild("_Index")
            :WaitForChild("sleitnick_net@0.2.0"):WaitForChild("net")
            :WaitForChild("RF/SellAllItems"):InvokeServer()
        print("‚úÖ Semua item dijual otomatis")

        task.wait(3) -- jeda 3 detik, ubah sesuai kebutuhan
    end
end)
]], [[
-- ==== BLOCK: ON (ketika toggle ON ‚Üí Script MATI) ====
_G.AutoSellEnabled = false
print("üõë Auto Sell dimatikan (toggle ON)")
]])




AddContent("Switch", "Auto AFK", [[
-- ==== BLOCK: OFF (toggle OFF ‚Üí Anti AFK AKTIF) ====
_G.AntiAFKEnabled = true
print("üü¢ AFK Active")

-- Hentikan loop lama kalau ada
if _G.AntiAFKLoop and coroutine.status(_G.AntiAFKLoop) ~= "dead" then
    task.cancel(_G.AntiAFKLoop)
end

local VirtualUser = game:GetService("VirtualUser")

_G.AntiAFKLoop = task.spawn(function()
    while _G.AntiAFKEnabled do
        game:GetService("Players").LocalPlayer.Idled:Wait()
        if not _G.AntiAFKEnabled then break end
        VirtualUser:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        task.wait(1)
        VirtualUser:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        print("üí§ Anti AFK aktif, kamu aman dari kick.")
    end
end)
]], [[
-- ==== BLOCK: ON (toggle ON ‚Üí Anti AFK NONAKTIF) ====
_G.AntiAFKEnabled = false
print("üî¥ AFK Nonaktif")
]])




AddContent("Switch", "Teleport", [[
-- ==== BLOCK: OFF (toggle OFF ‚Üí GUI DIBUKA) ====
print("üó∫Ô∏è Teleport GUI dibuka.")

pcall(function() game.CoreGui:FindFirstChild("TeleportGUI"):Destroy() end)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- GUI utama
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "TeleportGUI"
ScreenGui.Parent = game.CoreGui

-- Frame utama
local Frame = Instance.new("Frame")
Frame.Parent = ScreenGui
Frame.AnchorPoint = Vector2.new(1, 0)
Frame.Position = UDim2.new(1, -25, 0, 60)
Frame.Size = UDim2.new(0, 190, 0, 235)
Frame.BackgroundColor3 = Color3.fromRGB(28, 28, 28)
Frame.BackgroundTransparency = 0.1
Frame.BorderSizePixel = 0
Frame.Active = true
Frame.Draggable = true

-- Judul
local Title = Instance.new("TextLabel")
Title.Parent = Frame
Title.Size = UDim2.new(1, 0, 0, 30)
Title.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
Title.BorderSizePixel = 0
Title.Text = "üó∫Ô∏è SELECT AREA"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.SourceSansBold
Title.TextScaled = true

-- ScrollFrame
local Scroll = Instance.new("ScrollingFrame")
Scroll.Parent = Frame
Scroll.Position = UDim2.new(0, 0, 0, 32)
Scroll.Size = UDim2.new(1, 0, 1, -34)
Scroll.BackgroundTransparency = 1
Scroll.BorderSizePixel = 0
Scroll.ScrollBarThickness = 5
Scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y

-- Layout tombol
local ListLayout = Instance.new("UIListLayout")
ListLayout.Parent = Scroll
ListLayout.Padding = UDim.new(0, 6)
ListLayout.FillDirection = Enum.FillDirection.Vertical
ListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
ListLayout.SortOrder = Enum.SortOrder.LayoutOrder

-- Fungsi buat tombol teleport
local function CreateButton(name, cframe)
	local Btn = Instance.new("TextButton")
	Btn.Parent = Scroll
	Btn.AnchorPoint = Vector2.new(0.5, 0)
	Btn.Position = UDim2.new(0.5, 0, 0, 0)
	Btn.Size = UDim2.new(0, 155, 0, 26)
	Btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	Btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	Btn.Font = Enum.Font.SourceSansBold
	Btn.TextScaled = true
	Btn.BorderSizePixel = 0
	Btn.Text = name
	Btn.AutoButtonColor = true

	-- Hover efek
	Btn.MouseEnter:Connect(function()
		Btn.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
	end)
	Btn.MouseLeave:Connect(function()
		Btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	end)

	-- Teleport aksi
	Btn.MouseButton1Click:Connect(function()
		local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if hrp then
			hrp.CFrame = cframe
			print("üìç Teleported to " .. name)
		end
	end)
end

-- AREA LIST
CreateButton("Area 1", CFrame.new(
