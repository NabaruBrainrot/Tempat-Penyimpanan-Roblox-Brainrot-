loadstring(game:HttpGet("https://raw.githubusercontent.com/NabaruBrainrot/Tempat-Penyimpanan-Roblox-Brainrot-/refs/heads/main/test%20ui"))()
_G.FaDhenAddTitle("Beta Version")


_G.FaDhenAddToggle("Auto Fish", {
    Callback = function(state)
        print("Auto Fish:", state)

        if state then
            -- Kalau GUI sudah ada, munculkan lagi
            if game.CoreGui:FindFirstChild("DeliveryTouchGUI") then
                game.CoreGui.DeliveryTouchGUI.Enabled = true
                return
            end

            -- ==== BUAT GUI ====
            local TweenService = game:GetService("TweenService")
            local Players = game:GetService("Players")
            local LocalPlayer = Players.LocalPlayer
            local UserInputService = game:GetService("UserInputService")

            local ScreenGui = Instance.new("ScreenGui")    
            local Frame = Instance.new("Frame")    
            local Title = Instance.new("TextLabel")    
            local Button = Instance.new("TextButton")    
            local SliderFrame = Instance.new("Frame")
            local SliderBar = Instance.new("Frame")
            local SliderButton = Instance.new("TextButton")
            local ValueLabel = Instance.new("TextLabel")
            local UICorner_Frame = Instance.new("UICorner")    
            local UICorner_Button = Instance.new("UICorner")    
            local UICorner_Slider = Instance.new("UICorner")
            local UICorner_SliderBar = Instance.new("UICorner")
            local UICorner_SliderButton = Instance.new("UICorner")

            ScreenGui.Name = "DeliveryTouchGUI"    
            ScreenGui.Parent = game.CoreGui    
            ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
            ScreenGui.ResetOnSpawn = false

            Frame.Parent = ScreenGui    
            Frame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)    
            Frame.Position = UDim2.new(0, 535, 0, 11)    
            Frame.Size = UDim2.new(0, 140, 0, 110)    
            Frame.Active = true    
            Frame.Draggable = true
            Frame.BorderSizePixel = 0
            UICorner_Frame.CornerRadius = UDim.new(0, 10)    
            UICorner_Frame.Parent = Frame    

            Title.Parent = Frame
            Title.BackgroundTransparency = 1
            Title.Position = UDim2.new(0, 10, 0, 5)
            Title.Size = UDim2.new(0, 120, 0, 20)
            Title.Font = Enum.Font.GothamBold
            Title.Text = "Speed: 3.0"
            Title.TextColor3 = Color3.fromRGB(255, 255, 255)
            Title.TextSize = 12
            Title.TextXAlignment = Enum.TextXAlignment.Left

            -- Slider Background
            SliderFrame.Parent = Frame
            SliderFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            SliderFrame.Position = UDim2.new(0, 10, 0, 33)
            SliderFrame.Size = UDim2.new(0, 120, 0, 8)
            SliderFrame.BorderSizePixel = 0
            SliderFrame.ClipsDescendants = false
            UICorner_Slider.CornerRadius = UDim.new(1, 0)
            UICorner_Slider.Parent = SliderFrame

            -- Slider Bar (filled part)
            SliderBar.Parent = SliderFrame
            SliderBar.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
            SliderBar.Position = UDim2.new(0, 0, 0, 0)
            SliderBar.Size = UDim2.new(0.6, 0, 1, 0) -- Default 60% (value 3)
            SliderBar.BorderSizePixel = 0
            UICorner_SliderBar.CornerRadius = UDim.new(1, 0)
            UICorner_SliderBar.Parent = SliderBar

            -- Slider Button (draggable circle)
            SliderButton.Parent = SliderFrame
            SliderButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderButton.Position = UDim2.new(0.6, -8, 0.5, -8) -- Center on 60%
            SliderButton.Size = UDim2.new(0, 16, 0, 16)
            SliderButton.Text = ""
            SliderButton.AutoButtonColor = false
            SliderButton.BorderSizePixel = 0
            SliderButton.ZIndex = 2
            UICorner_SliderButton.CornerRadius = UDim.new(1, 0)
            UICorner_SliderButton.Parent = SliderButton

            -- Value Label (sekarang kosong/tersembunyi)
            ValueLabel.Parent = Frame
            ValueLabel.BackgroundTransparency = 1
            ValueLabel.Position = UDim2.new(0, 10, 0, 42)
            ValueLabel.Size = UDim2.new(0, 120, 0, 15)
            ValueLabel.Font = Enum.Font.GothamBold
            ValueLabel.Text = ""
            ValueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            ValueLabel.TextSize = 11
            ValueLabel.TextXAlignment = Enum.TextXAlignment.Center
            ValueLabel.Visible = false

            Button.Parent = Frame    
            Button.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
            Button.Position = UDim2.new(0, 10, 0, 65)
            Button.Size = UDim2.new(0, 120, 0, 30)
            Button.Font = Enum.Font.GothamBold
            Button.Text = "Mulai"
            Button.TextColor3 = Color3.fromRGB(255, 255, 255)
            Button.TextSize = 12
            Button.BorderSizePixel = 0
            UICorner_Button.CornerRadius = UDim.new(0, 8)
            UICorner_Button.Parent = Button

            -- ==== SLIDER LOGIC WITH SAVE DATA ====
            local minValue = 0
            local maxValue = 5
            local currentValue = 3
            local dragging = false

            -- Load saved slider value
            local function loadSliderValue()
                local success, result = pcall(function()
                    return game:GetService("HttpService"):JSONDecode(readfile("AutoFishSliderValue.json"))
                end)
                
                if success and result and result.speed then
                    currentValue = math.clamp(result.speed, minValue, maxValue)
                    print("‚úÖ Loaded slider value:", currentValue)
                else
                    currentValue = 3 -- Default value
                    print("‚ÑπÔ∏è Using default slider value:", currentValue)
                end
            end

            -- Save slider value
            local function saveSliderValue()
                local success, err = pcall(function()
                    local data = {speed = currentValue}
                    writefile("AutoFishSliderValue.json", game:GetService("HttpService"):JSONEncode(data))
                end)
                
                if success then
                    print("üíæ Slider value saved:", currentValue)
                else
                    warn("‚ùå Failed to save slider value:", err)
                end
            end

            -- Load value saat GUI dibuat
            loadSliderValue()

            -- Fungsi untuk update slider
            local function updateSlider(input)
                local sliderPos = SliderFrame.AbsolutePosition.X
                local sliderSize = SliderFrame.AbsoluteSize.X
                local mousePos = input.Position.X
                
                -- Hitung posisi relatif
                local relativePos = math.clamp(mousePos - sliderPos, 0, sliderSize)
                local percentage = relativePos / sliderSize
                
                -- Update value
                currentValue = minValue + (maxValue - minValue) * percentage
                currentValue = math.floor(currentValue * 10) / 10 -- Round to 1 decimal
                
                -- Update posisi button dan bar
                SliderButton.Position = UDim2.new(percentage, -8, 0.5, -8)
                SliderBar.Size = UDim2.new(percentage, 0, 1, 0)
                Title.Text = string.format("Speed: %.1f", currentValue)
            end

            -- Event untuk mulai drag
            SliderButton.MouseButton1Down:Connect(function()
                dragging = true
                SliderButton.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
            end)

            -- Event untuk stop drag
            UserInputService.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or 
                   input.UserInputType == Enum.UserInputType.Touch then
                    if dragging then
                        dragging = false
                        SliderButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        -- Save value saat selesai drag
                        saveSliderValue()
                    end
                end
            end)

            -- Event untuk update saat drag
            UserInputService.InputChanged:Connect(function(input)
                if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or
                   input.UserInputType == Enum.UserInputType.Touch) then
                    updateSlider(input)
                end
            end)

            -- Klik langsung pada slider frame
            SliderFrame.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or
                   input.UserInputType == Enum.UserInputType.Touch then
                    dragging = true
                    SliderButton.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
                    updateSlider(input)
                end
            end)

            -- Animasi hover
            SliderButton.MouseEnter:Connect(function()
                if not dragging then
                    TweenService:Create(SliderButton, TweenInfo.new(0.2), {
                        BackgroundColor3 = Color3.fromRGB(220, 220, 220),
                        Size = UDim2.new(0, 18, 0, 18)
                    }):Play()
                end
            end)

            SliderButton.MouseLeave:Connect(function()
                if not dragging then
                    TweenService:Create(SliderButton, TweenInfo.new(0.2), {
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        Size = UDim2.new(0, 16, 0, 16)
                    }):Play()
                end
            end)

            -- Initialize slider position dengan saved value
            local initPercentage = (currentValue - minValue) / (maxValue - minValue)
            SliderButton.Position = UDim2.new(initPercentage, -8, 0.5, -8)
            SliderBar.Size = UDim2.new(initPercentage, 0, 1, 0)
            Title.Text = string.format("Speed: %.1f", currentValue)

            -- ====== TOGGLE & AUTO FISHING ======
            local isOn = false
            _G.AutoFishingEnabled = false
            _G.AutoFishingLoop = nil

            local function startFishingLoop()
                _G.AutoFishingEnabled = true
                print("üé£ Auto Fishing Dinyalakan")

                if _G.AutoFishingLoop and coroutine.status(_G.AutoFishingLoop) ~= "dead" then
                    task.cancel(_G.AutoFishingLoop)
                end

                if not _G.OriginalCFrame and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    _G.OriginalCFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
                end

                _G.AutoFishingLoop = task.spawn(function()
                    local lastResetTime = os.clock()
                    while _G.AutoFishingEnabled do
                        local now = os.clock()
                        local waitTime = currentValue

                        if now - lastResetTime >= 120 then
                            local player = LocalPlayer
                            if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                                local lastPos = player.Character.HumanoidRootPart.CFrame
                                print("‚ôªÔ∏è Reset karakter di lokasi saat ini...")
                                player.Character:FindFirstChildOfClass("Humanoid"):TakeDamage(9999)
                                player.CharacterAdded:Wait()
                                repeat task.wait() until player.Character:FindFirstChild("HumanoidRootPart")
                                player.Character:WaitForChild("HumanoidRootPart").CFrame = lastPos
                            end
                            lastResetTime = os.clock()
                        end

                        local args1 = { 1 }
                        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
                            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
                            :WaitForChild("net"):WaitForChild("RE/EquipToolFromHotbar")
                            :FireServer(unpack(args1))
                        task.wait(0.1)

                        local args2 = { 1760498391.426017 }
                        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
                            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
                            :WaitForChild("net"):WaitForChild("RF/ChargeFishingRod")
                            :InvokeServer(unpack(args2))
                        task.wait(0.1)

                        local args3 = { -0.5718746185302734, 0.9997670786545603 }
                        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
                            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
                            :WaitForChild("net"):WaitForChild("RF/RequestFishingMinigameStarted")
                            :InvokeServer(unpack(args3))

                        task.wait(waitTime)

                        game:GetService("ReplicatedStorage"):WaitForChild("Packages")
                            :WaitForChild("_Index"):WaitForChild("sleitnick_net@0.2.0")
                            :WaitForChild("net"):WaitForChild("RE/FishingCompleted")
                            :FireServer()

                        if not _G.AutoFishingEnabled then
                            break
                        end
                    end
                end)
            end

            local function stopFishingLoop()
                _G.AutoFishingEnabled = false
                print("üõë Auto Fishing Diminta Berhenti")
            end

            Button.MouseButton1Click:Connect(function()
                isOn = not isOn
                if isOn then
                    Button.Text = "Berhenti"
                    TweenService:Create(Button, TweenInfo.new(0.25), {BackgroundColor3 = Color3.fromRGB(0, 200, 120)}):Play()
                    startFishingLoop()
                else
                    Button.Text = "Mulai"
                    TweenService:Create(Button, TweenInfo.new(0.25), {BackgroundColor3 = Color3.fromRGB(0, 170, 255)}):Play()
                    stopFishingLoop()
                end
            end)

        else

            if game.CoreGui:FindFirstChild("DeliveryTouchGUI") then
                game.CoreGui.DeliveryTouchGUI.Enabled = false
            end
            print("üõë GUI Auto Fishing disembunyikan, loop tetap sesuai tombol GUI.")
        end
    end
})







_G.FaDhenAddToggle("Auto Sell", {
    Callback = function(state)

        if state then

            _G.AutoSellEnabled = true
            print("üí∞ Auto Sell diaktifkan (toggle ON)")

            
            if _G.AutoSellLoop and coroutine.status(_G.AutoSellLoop) ~= "dead" then
                task.cancel(_G.AutoSellLoop)
            end


            _G.AutoSellLoop = task.spawn(function()
                while _G.AutoSellEnabled do
                    local ReplicatedStorage = game:GetService("ReplicatedStorage")
                    ReplicatedStorage:WaitForChild("Packages")
                        :WaitForChild("_Index")
                        :WaitForChild("sleitnick_net@0.2.0")
                        :WaitForChild("net")
                        :WaitForChild("RF/SellAllItems")
                        :InvokeServer()

                    print("‚úÖ Semua item dijual otomatis")
                    task.wait(1) 
                end
            end)
        else
            -- ==== Toggle OFF ‚Üí Matikan Auto Sell ====
            _G.AutoSellEnabled = false
            print("üõë Auto Sell dimatikan (toggle OFF)")
        end
    end
})




_G.FaDhenAddToggle("Anti AFK", {
    Callback = function(state)

        if state then
            -- ==== Toggle ON ‚Üí Aktifkan Anti AFK ====
            _G.AntiAFKEnabled = true
            print("üü¢ Anti AFK Aktif")

            -- Hentikan loop lama kalau ada
            if _G.AntiAFKLoop and coroutine.status(_G.AntiAFKLoop) ~= "dead" then
                task.cancel(_G.AntiAFKLoop)
            end

            local VirtualUser = game:GetService("VirtualUser")

            _G.AntiAFKLoop = task.spawn(function()
                while _G.AntiAFKEnabled do
                    game:GetService("Players").LocalPlayer.Idled:Wait()
                    if not _G.AntiAFKEnabled then break end

                    VirtualUser:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
                    task.wait(1)
                    VirtualUser:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)

                    print("üí§ Anti AFK aktif, kamu aman dari kick.")
                end
            end)

        else
            -- ==== Toggle OFF ‚Üí Matikan Anti AFK ====
            _G.AntiAFKEnabled = false
            print("üî¥ Anti AFK Dimatikan")
        end
    end
})




_G.FaDhenAddToggle("Auto Fly", {
    Callback = function(state)
        print("Auto Fly:", state)

        if state then
            -- Saat Toggle ON ‚Üí Jalankan Script Fly
            loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
        else
            -- Saat Toggle OFF ‚Üí (kosong, bisa tambahkan stop fly di sini jika mau)
            print("Auto Fly dimatikan")
        end
    end
})


